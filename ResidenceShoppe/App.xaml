<Application x:Class="ResidenceShoppe.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Residence.ViewModels;assembly=Residence.ViewModels"
             xmlns:views = "clr-namespace:ResidenceShoppe">
    <Application.Resources>
        <DataTemplate DataType="{x:Type local:MainViewModel}">
            <views:MainWindow />
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:EditViewModel}">
            <views:EditView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:AddViewModel}">
            <views:AddView />
        </DataTemplate>
    </Application.Resources>
</Application>
